{
	  "manifest_version": 2,
	  "name": "Search Auto Open",
	  "version": "0.1",
	  "content_scripts": [
			{
				"matches": [
					"https://www.google.co.in/*"
				],
				"js": ["jquery-3.1.1.min.js", "content.js"]
			}
	  ],
		"background": {
			"scripts": ["background.js"]
		},
		"browser_action": {
			"default_icon": "icon.png"
		}
}
